{
  "name": "webpack-express-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate for running a Webpack workflow in Node express",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "build": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "dev": "webpack-dev-server --history-api-fallback --devtool eval --progress --colors --content-base build",
    "test-dev": "webpack-dev-server --history-api-fallback --devtool eval --progress --colors --content-base testBuild",
    "postinstall": "node heroku",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christianalfoni/webpack-express-boilerplate.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node"
  ],
  "author": "Christian Alfoni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christianalfoni/webpack-express-boilerplate/issues"
  },
  "homepage": "https://github.com/christianalfoni/webpack-express-boilerplate",
  "dependencies": {
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "css-loader": "^0.10.1",
    "express": "^4.12.3",
    "html-loader": "^0.3.0",
    "http-proxy": "^1.10.1",
    "markdown-loader": "^0.1.3",
    "material-ui": "^0.8.0",
    "mousetrap": "^1.5.3",
    "react-side-effect-mousetrap": "thr1ve/react-side-effect-mousetrap",
    "style-loader": "^0.10.2",
    "webpack": "^1.8.5",
    "webpack-dev-server": "^1.8.0"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.26",
    "eslint": "^0.24.1",
    "eslint-config-standard": "^3.4.1",
    "eslint-plugin-react": "^3.2.0",
    "standard": "^5.0.0"
  }
}
